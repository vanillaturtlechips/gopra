{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "api/posts/index.go", 
      "use": "@vercel/go"
    },
    {
      "src": "api/upload-request/index.go", 
      "use": "@vercel/go"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/api/posts(.*)",
      "dest": "api/posts/index.go" 
    },
    {
      "src": "/api/upload-request",
      "dest": "api/upload-request/index.go" 
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}